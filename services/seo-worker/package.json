{
  "name": "@pharmaiq/seo-worker",
  "version": "1.0.0",
  "description": "SEO optimization worker for PharmaIQ",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js",
    "start:dev": "tsx watch src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@pharmaiq/types": "file:../../shared/types",
    "@pharmaiq/contracts": "file:../../shared/contracts",
    "@pharmaiq/utils": "file:../../shared/utils",
    "bull": "^4.11.0",
    "ioredis": "^5.3.2",
    "express": "^4.18.0",
    "sitemap": "^7.1.0",
    "schema-dts": "^1.1.0",
    "zod": "^3.22.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/*.spec.ts", "**/*.test.ts"]
  }
}